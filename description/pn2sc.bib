@inproceedings{vanhooff05transformationchain,
	abstract = {The Model Driven Development (MDD) paradigm stimulates the use of models as the main artifacts for software development. These models can be situated at high levels of abstraction, close to the applications business domain. Many consecutive automatic transformations (a transformation chain) can be applied to these models to add the necessary details in order to generate a concrete implementation. This means that a large part of the total development effort is relocated to the development of transformations and hence we should have the necessary tooling support for designing transformation chains. In this paper we propose a metamodel for a transformation chain modeling language that enables implementation independent composition of transformations. We also propose a concrete syntax for this language that is based on UML activity diagrams.},
	author = {Vanhooff, Bert   and Van Baelen, Stefan   and Hovsepyan, Aram   and Joosen, Wouter   and Berbers, Yolande  },
	citeulike-article-id = {1615887},
	doi = {10.1007/11796435\_6},
	booktitle = {Embedded Computer Systems: Architectures, Modeling, and Simulation ({SAMOS VI})},
	location={Samos, Greece},
	pages = {39--48},
	title = {Towards a Transformation Chain Modeling Language},
	doi = {10.1007/11796435_6},
	year = {2006}
}

